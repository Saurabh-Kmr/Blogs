[32m+[m[32m})[m
[32m+[m[32mapp.post('/compose',function(req,resp){[m
[32m+[m
[32m+[m[32m  const post= new Post({[m
[32m+[m[32m    title:req.body.postTitle,[m
[32m+[m[32m    content:req.body.postBody})[m
