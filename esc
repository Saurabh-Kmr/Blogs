[32m+[m[32m    post.save()[m
[32m+[m[32m    console.log(post._id)[m
[32m+[m[32m    resp.redirect('/')[m
[32m+[m[32m  }[m
[32m+[m[32m  catch(err)[m
[32m+[m[32m  {[m
[32m+[m[32m    resp.render('Errors',{error:err})[m
[32m+[m[32m    setTimeout(()=>resp.redirect('/'),3000)[m
[32m+[m[32m  }[m
[32m+[m[32m  // resp.redirect('/')[m
[32m+[m[32m  // console.log(posts)[m
[32m+[m[32m})[m
