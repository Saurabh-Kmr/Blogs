[32m+[m[32mconst express = require("express")[m
[32m+[m
[32m+[m[32mconst router = express.Router()[m
[32m+[m
[32m+[m[32mconst Post = require('../models/post')[m
[32m+[m
[32m+[m[32mrouter.get('/:postId',async (req,resp) =>{[m
[32m+[m[32m  const posts=req.params.postId[m
[32m+[m[32m  console.log(posts)[m
[32m+[m[32m  try{[m
[32m+[m[32m    const post= await Post.findOne({_id:posts})[m
[32m+[m[32m    if (post==null){[m
[32m+[m[32m      resp.render('Errors',{error:'404 not found'})[m
[32m+[m[32m    }[m
[32m+[m[32m    else{[m
[32m+[m[32m     console.log(post)[m
[32m+[m[32m     resp.render('post',{Title:post.title,content:post.content})[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  catch(err){[m
[32m+[m[32m    resp.render('Errors',{error:err})[m
[32m+[m[32m  }[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mmodule.exports= router[m
