[32m+[m[32m  console.log(post)[m
[32m+[m[32m  posts.forEach((element)=>{[m
[32m+[m
[32m+[m[32m    if(post===_.lowerCase(element.postTitle)){[m
[32m+[m[32m      resp.render('post',{Title:element.postTitle,content:element.postBody});[m
[32m+[m[32m    }[m
[32m+[m[32m    // else{[m
